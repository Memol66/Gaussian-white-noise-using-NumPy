import numpy as np.
import matplotlib.pyplot as plt

# Number of samples 00000000000000000
num_samples = 1000

# Generating Gaussian white noise.
white_noise = np.random.randn(num_samples)

# Plotting the generated Gaussian white noise.
plt.plot(white_noise)
plt.xlabel('Time')
plt.ylabel('Noise Value')
plt.title('Gaussian White Noise')
plt.show()
